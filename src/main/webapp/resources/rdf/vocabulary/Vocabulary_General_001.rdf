	<skos:ConceptScheme rdf:about="http://appsnserv.testbed.fi-ware.eu/cloudservices/rdf/vocabulary/Vocabulary_General_001">
		<skos:prefLabel xml:lang="en">General service resource/property vocabulary</skos:prefLabel>
		<skos:altLabel xml:lang="en">General vocabulary</skos:altLabel>
		<skos:definition>Specification of the general vocabulary to describe service resources and properties.</skos:definition>
		<dcterms:contributor>
			<foaf:Person>
				<foaf:name>Florian Heberle</foaf:name>
				<foaf:firstName>Florian</foaf:firstName>
