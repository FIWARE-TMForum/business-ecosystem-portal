:slp_Rackspace a usdl:ServiceLevelProfile ;
	dcterms:title "Service Level Profile for Rackspace's Cloud Servers"@en ;
	dcterms:description "All information integrated here are not legally binding. SLAs can be accessed on our website under 'http://www.rackspace.com/cloud/legal/sla/'. This Service Level Guaranty is your sole and exclusive remedy for Cloud Servers unavailability. Notwithstanding anything in this Service Level Guaranty to the contrary, the maximum total credit for the monthly billing period, including all guaranties, shall not exceed 100% of your fee for that billing period. Credits that would be available but for this limitation will not be carried forward to future billing periods. This Service Level Guaranty is part of your Agreement with The Rackspace Cloud, along with The Rackspace Cloud Terms of Service and the AUP, and is subject to the terms and conditions stated in those documents."@en ;
	sla:hasServiceLevel 
		:slo_Rackspace_Availability_Network ,
		:slo_Rackspace_Availability_Network_Exclusion ,
		:slo_Rackspace_Availability_DataCentreInfrastructure ,
		:slo_Rackspace_Availability_DataCentreInfrastructure_Exclusion ,
		:slo_Rackspace_CloudServerHostsRTO ,
		:slo_Rackspace_Migration ,
		:slo_Rackspace_AvailibilityCalculation_Network ,
		:slo_Rackspace_AvailibilityCalculation_DataCenterInfrastructure ,
		:slo_Rackspace_AvailibilityCalculation_CloudServerHosts ,
		:slo_Rackspace_AvailibilityCalculation_Migration ,
		:slo_Rackspace_CreditSuspension ,
		:slo_Rackspace_CreditRequest .

#____________________________________________________________________________________
:slo_Rackspace_Availability_Network a sla:GuaranteedState ;
	dcterms:title "Availability: Network"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "We guaranty that our data center network will be available 100% of the time in any given monthly billing period."@en ;
			sla:hasVariable :var_Rackspace_Availability_Network_Value ] .

:var_Rackspace_Availability_Network_Value a sla:Variable ;
	rdfs:label "Network availability value"@en ;
	sla:hasDefault 
		[ a :Rackspace_NetworkAvailability ;
			gr:hasValue "100" ;

			gr:hasUnitOfMeasurement "P1" ] .

#____________________________________________________________________________________
:slo_Rackspace_Availability_Network_Exclusion a sla:GuaranteedState ;
	dcterms:title "Availability exclusion: Network"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Data center network availability is not influenced by scheduled maintenance."@en ;
			sla:hasVariable :var_Rackspace_Availability_Network_Exclusion_ScheduledMaintenance ] .

:var_Rackspace_Availability_Network_Exclusion_ScheduledMaintenance a sla:Variable ;
	rdfs:label "Scheduled Maintenance"@en ;
	sla:hasDefault [ a :Rackspace_NetworkAvailability_Exclusion ] .

#____________________________________________________________________________________
:slo_Rackspace_Availability_DataCentreInfrastructure a sla:GuaranteedState ;
	dcterms:title "Availability: Data centre infrastructure"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "We guaranty that data center HVAC and power will be functioning 100% of the time in any given monthly billing period, excluding scheduled maintenance. Infrastructure downtime exists when Cloud Servers downtime occurs as a result of power or heat problems."@en ;
			sla:hasVariable :var_Rackspace_Availability_DataCentreInfrastructure_Value ] .

:var_Rackspace_Availability_DataCentreInfrastructure_Value a sla:Variable ;
	rdfs:label "Data centre infrastructure availability value"@en ;
	sla:hasDefault 
		[ a :Rackspace_DataCenterInfrastructureAvailability ;
			gr:hasValue "100" ;
			gr:hasUnitOfMeasurement "P1" ] .

#____________________________________________________________________________________
:slo_Rackspace_Availability_DataCentreInfrastructure_Exclusion a sla:GuaranteedState ;
	dcterms:title "Availability exclusion: Data centre infrastructure"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Data center infrastructure availability is not influenced by scheduled maintenance."@en ;
			sla:hasVariable :var_Rackspace_Availability_Network_Exclusion_ScheduledMaintenance ] .

:var_Rackspace_Availability_DataCentreInfrastructure_Exclusion_ScheduledMaintenance a sla:Variable ;
	rdfs:label "Scheduled Maintenance"@en ;
	sla:hasDefault [ a :Rackspace_DataCentreInfrastructure_Exclusion ] .

#____________________________________________________________________________________
:slo_Rackspace_CloudServerHostsRTO a sla:GuaranteedState ;
	dcterms:title "Cloud Server Hosts RTO"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "We guaranty the functioning of all cloud server hosts including compute, storage, and hypervisor. If a cloud server host fails, we guaranty that restoration or repair will be complete within one hour of problem identification."@en ;
			sla:hasVariable :var_Rackspace_CloudServerHostsRTO_Value ] .

:var_Rackspace_CloudServerHostsRTO_Value a sla:Variable ;
	rdfs:label "Cloud Server Hosts recovery time objective (RTO)"@en ;
	sla:hasDefault 
		[ a cloud:RecoveryTimeObjective ;
			gr:hasValue "1" ;
			gr:hasUnitOfMeasurement "HUR" ] .

#____________________________________________________________________________________
:slo_Rackspace_Migration a sla:GuaranteedState ;
	dcterms:title "Migration"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "If a cloud server migration is required because of cloud server host degradation, we will notify you at least 24 hours in advance of beginning the migration, unless we determine in our reasonable judgment, that we must begin the migration sooner to protect your cloud server data. Either way, we guaranty that the migration will be complete within three hours of the time that we begin the migration."@en ;
			sla:hasVariable 
				:var_Rackspace_Migration_Notification , 
				:var_Rackspace_Migration_Duration ] .

:var_Rackspace_Migration_Notification a sla:Variable ;
	rdfs:label "Notification at least 24h in advance"@en ;
	sla:hasDefault 
		[ a genVoc:AbsoluteTimePeriod ;
			gr:hasMinValue "24" ;
			gr:hasUnitOfMeasurement "HUR" ] .

:var_Rackspace_Migration_Duration a sla:Variable ;
	rdfs:label "Duration of migration max 3h"@en ;
	sla:hasDefault 
		[ a genVoc:AbsoluteTimePeriod ;
			gr:hasMaxValue "3" ;
			gr:hasUnitOfMeasurement "HUR" ] .

#____________________________________________________________________________________
:slo_Rackspace_AvailibilityCalculation_Network a sla:GuaranteedState ;
	dcterms:title "Crediting: Network"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "If we fail to meet a guaranty for network availability, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle)."@en ;
			sla:hasVariable	:var_Rackspace_AvailibilityCalculation_Network_Slice ] .
				
:var_Rackspace_AvailibilityCalculation_Network_Slice a sla:Variable ;
	rdfs:label "Credit equal to 5% credit of the fees for each 30 minutes of downtime, up to 100%."@en ;
	sla:hasDefault 
		[ a :Rackspace_CreditPeriodSlice_Network ;
			gr:hasValue "30" ;
			gr:hasUnitOfMeasurement "MIN" ] .

#____________________________________________________________________________________
:slo_Rackspace_AvailibilityCalculation_DataCenterInfrastructure a sla:GuaranteedState ;
	dcterms:title "Crediting: Data center infrastructure"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "If we fail to meet a guaranty for infrastructure availability, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle)."@en ;
			sla:hasVariable :var_Rackspace_AvailibilityCalculation_DataCenterInfrastructure_Slice ] .
				
:var_Rackspace_AvailibilityCalculation_DataCenterInfrastructure_Slice a sla:Variable ;
	rdfs:label "Credit equal to 5% credit of the fees for each 30 minutes of downtime, up to 100%."@en ;
	sla:hasDefault 
		[ a :Rackspace_CreditPeriodSlice_DataCenterInfrastructure ;
			gr:hasValue "30" ;
			gr:hasUnitOfMeasurement "MIN" ] .

#____________________________________________________________________________________
:slo_Rackspace_AvailibilityCalculation_CloudServerHosts a sla:GuaranteedState ;
	dcterms:title "Crediting: Cloud Server Hosts RTO"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "If we fail to meet a guaranty for Cloud Server Hosts RTO, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle)."@en ;
			sla:hasVariable	:var_Rackspace_AvailibilityCalculation_CloudServerHosts_Value ] .
							
:var_Rackspace_AvailibilityCalculation_CloudServerHosts_Value a sla:Variable ;
	rdfs:label "Credit equal to 5% of the fees for each additional hour of downtime, up to 100%."@en ;
	sla:hasDefault 
		[ a :Rackspace_CreditPeriodSlice_CloudServerHosts ;
			gr:hasValue "1" ;
			gr:hasUnitOfMeasurement "HUR" ] .

#____________________________________________________________________________________	
:slo_Rackspace_AvailibilityCalculation_Migration a sla:GuaranteedState ;
	dcterms:title "Crediting: Migration"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "If we fail to meet a guaranty for duration of migration, you will be eligible for a credit. Credits will be calculated as a percentage of the fees for the Cloud Servers adversely affected by the failure for the current monthly billing period during which the failure occurred (to be applied at the end of the billing cycle)."@en ;
			sla:hasVariable :var_Rackspace_AvailibilityCalculation_Migration_Value ] .
				
:var_Rackspace_AvailibilityCalculation_Migration_Value a sla:Variable ;
	rdfs:label "Credit equal to 5% of the fees for each additional hour of downtime, up to 100%."@en ;
	sla:hasDefault 
		[ a :Rackspace_CreditPeriodSlice_Migration ;
			gr:hasValue "1" ;
			gr:hasUnitOfMeasurement "HUR" ] .

#____________________________________________________________________________________	
:slo_Rackspace_CreditSuspension a sla:GuaranteedState ;
	dcterms:title "Credit suspension"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "Reasons for the suspension of credit entitlement."@en ;
			sla:hasVariable 
				:var_Rackspace_CreditSuspension_SLABreach ,
				:var_Rackspace_CreditSuspension_AgreementBreach ,
				:var_Rackspace_CreditSuspension_Misuese ] .

:var_Rackspace_CreditSuspension_SLABreach a sla:Variable ;
	rdfs:label "You are not entitled to a credit if you are in breach of your services agreement with The Rackspace Cloud (including your payment obligations to us) until you have cured the breach."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

:var_Rackspace_CreditSuspension_AgreementBreach a sla:Variable ;
	rdfs:label "You are not entitled to a credit if downtime would not have occurred but for your breach of your agreement with The Rackspace Cloud."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

:var_Rackspace_CreditSuspension_Misuese a sla:Variable ;
	rdfs:label "You are not entitled to a credit if downtime would not have occurred but for your misuse of Cloud Servers system."@en ;
	sla:hasDefault [ a cloud:CreditSuspensionReason ] .

#____________________________________________________________________________________
:slo_Rackspace_CreditRequest a sla:GuaranteedAction ;
	dcterms:title "Credit request"@en ;
	sla:serviceLevelExpression 
		[ a sla:ServiceLevelExpression ;
			dcterms:description "To receive a credit, you must contact your The Rackspace Cloud account team within thirty (30) days following the end of the downtime. You must show that your use of the Cloud Servers was adversely affected in some way as a result of the downtime to be eligible for the credit."@en ;
			sla:hasVariable 
				:var_Rackspace_CreditRequest_CreditRequestPeriod ] .

:var_Rackspace_CreditRequest_CreditRequestPeriod a sla:Variable ;
	rdfs:label "Request period"@en ;
	sla:hasDefault 
		[ a cloud:CreditRequestPeriod ;
			gr:hasValue "30" ;
			gr:hasUnitOfMeasurement "DAY" ] . 

