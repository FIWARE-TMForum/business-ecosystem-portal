					<sla:GuaranteedState rdf:about="http://appsnserv.testbed.fi-ware.eu/cloudservices/rdf/cloudServices/Amazon_EC2_001#slo_EC2_AvailibilityCalculation">
						<dcterms:title xml:lang="en">Annual Uptime Percentage calculation</dcterms:title>
						<sla:serviceLevelExpression>
							<sla:ServiceLevelExpression>
